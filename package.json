{
  "name": "minimarket-api",
  "version": "1.0.0",
  "description": "API REST para Sistema de Minimarket Intermedio PRO",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "cross-env NODE_ENV=test jest --detectOpenHandles --runInBand",
    "test:smoke": "cross-env NODE_ENV=test jest tests/backend/smoke.test.js --detectOpenHandles --runInBand",
    "test:journey": "cross-env NODE_ENV=test jest tests/backend/journey.test.js --detectOpenHandles --runInBand",
    "test:all": "cross-env NODE_ENV=test jest --detectOpenHandles --runInBand",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage --detectOpenHandles --runInBand",
    "lint": "eslint src/",
    "migrate": "node scripts/migrate.js"
  },
  "keywords": [
    "pos",
    "minimarket",
    "inventory",
    "cash-management",
    "qr-payments"
